{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "new-cap": 0,
    "no-loop-func": 0,
    "no-unreachable": 0,
    "no-process-exit": 0,
    "no-unused-vars": [1, {"args": "none"}],
    "semi": [2, "always"],
    "no-extra-semi": 1,

    "react/jsx-no-undef": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-multi-comp": 0,
    "react/no-unknown-property": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 2,
    "react/no-did-mount-set-state" : 2,
    "react/no-did-update-set-state" : 2,
  },
  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "es6": true,
    "mocha": true,
  },
  "plugins": ["react"]
}
